    "import datetime as dt\n",
    "import yfinance as yf\n",
    "\n",
    "company = 'NFLX'\n",
    "\n",
    "# Define a start date and End Date\n",
    "start = dt.datetime(2020,1,1)\n",
    "end =  dt.datetime(2022,1,1)\n",
    "\n",
    "# Read Stock Price Data \n",
    "data = yf.download(company, start , end)\n",
    "\n",
    "data.tail(10)"
